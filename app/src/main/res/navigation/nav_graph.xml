<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/indexFragment">

    <fragment
        android:id="@+id/indexFragment"
        android:name="com.utad.newComponets.IndexFragment"
        android:label="fragment_index"
        tools:layout="@layout/fragment_index" >
        <action
            android:id="@+id/action_indexFragment_to_switchFragment"
            app:destination="@id/switchFragment" />
        <action
            android:id="@+id/action_indexFragment_to_coordinatorAndCollapsingToolbarFragment"
            app:destination="@id/coordinatorAndCollapsingToolbarFragment" />
        <action
            android:id="@+id/action_indexFragment_to_scrollsFragment"
            app:destination="@id/scrollsFragment" />
        <action
            android:id="@+id/action_indexFragment_to_toolbarFragment"
            app:destination="@id/toolbarFragment" />
        <action
            android:id="@+id/action_indexFragment_to_checkBoxFragment"
            app:destination="@id/checkBoxFragment" />
        <action
            android:id="@+id/action_indexFragment_to_buttonsFragment"
            app:destination="@id/buttonsFragment" />
        <action
            android:id="@+id/action_indexFragment_to_cardViewFragment"
            app:destination="@id/cardViewFragment" />
        <action
            android:id="@+id/action_indexFragment_to_fabFragment"
            app:destination="@id/fabFragment" />
        <action
            android:id="@+id/action_indexFragment_to_recyclerViewFragment"
            app:destination="@id/recyclerViewFragment" />
        <action
            android:id="@+id/action_indexFragment_to_recyclerViewWithListAdapterFragment"
            app:destination="@id/recyclerViewWithListAdapterFragment" />
    </fragment>
    <fragment
        android:id="@+id/switchFragment"
        android:name="com.utad.newComponets.components.SwitchFragment"
        android:label="fragment_switch"
        tools:layout="@layout/fragment_switch" />
    <fragment
        android:id="@+id/cardViewFragment"
        android:name="com.utad.newComponets.components.CardViewFragment"
        android:label="fragment_card_view"
        tools:layout="@layout/fragment_card_view" />
    <fragment
        android:id="@+id/buttonsFragment"
        android:name="com.utad.newComponets.components.ButtonsFragment"
        android:label="fragment_buttons"
        tools:layout="@layout/fragment_buttons" />
    <fragment
        android:id="@+id/checkBoxFragment"
        android:name="com.utad.newComponets.components.CheckBoxFragment"
        android:label="fragment_check_box"
        tools:layout="@layout/fragment_check_box" />
    <fragment
        android:id="@+id/coordinatorAndCollapsingToolbarFragment"
        android:name="com.utad.newComponets.components.CoordinatorAndCollapsingToolbarFragment"
        android:label="fragment_coordinator_and_collapsing_toolbar"
        tools:layout="@layout/fragment_coordinator_and_collapsing_toolbar" />
    <fragment
        android:id="@+id/scrollsFragment"
        android:name="com.utad.newComponets.components.ScrollsFragment"
        android:label="fragment_scrolls"
        tools:layout="@layout/fragment_scrolls" >
        <action
            android:id="@+id/action_scrollsFragment_to_scrollViewFragment"
            app:destination="@id/scrollViewFragment" />
        <action
            android:id="@+id/action_scrollsFragment_to_nestedScrollViewFragment"
            app:destination="@id/nestedScrollViewFragment" />
        <action
            android:id="@+id/action_scrollsFragment_to_horizontalScrollFragment"
            app:destination="@id/horizontalScrollFragment" />
    </fragment>
    <fragment
        android:id="@+id/toolbarFragment"
        android:name="com.utad.newComponets.components.ToolbarFragment"
        android:label="fragment_toolbar"
        tools:layout="@layout/fragment_toolbar" />
    <fragment
        android:id="@+id/fabFragment"
        android:name="com.utad.newComponets.components.FabFragment"
        android:label="fragment_fab"
        tools:layout="@layout/fragment_fab" />
    <fragment
        android:id="@+id/horizontalScrollFragment"
        android:name="com.utad.newComponets.components.scrollViews.HorizontalScrollFragment"
        android:label="fragment_horizontal_scroll"
        tools:layout="@layout/fragment_horizontal_scroll" />
    <fragment
        android:id="@+id/nestedScrollViewFragment"
        android:name="com.utad.newComponets.components.scrollViews.NestedScrollViewFragment"
        android:label="fragment_nested_scroll_view"
        tools:layout="@layout/fragment_nested_scroll_view" />
    <fragment
        android:id="@+id/scrollViewFragment"
        android:name="com.utad.newComponets.components.scrollViews.ScrollViewFragment"
        android:label="fragment_scroll_view"
        tools:layout="@layout/fragment_scroll_view" />
    <fragment
        android:id="@+id/recyclerViewFragment"
        android:name="com.utad.newComponets.recyclerView.RecyclerViewFragment"
        android:label="fragment_recycler_view"
        tools:layout="@layout/fragment_recycler_view" />
    <fragment
        android:id="@+id/recyclerViewWithListAdapterFragment"
        android:name="com.utad.newComponets.recyclerView.RecyclerViewWithListAdapterFragment"
        android:label="fragment_recycler_view_with_list_adapter"
        tools:layout="@layout/fragment_recycler_view_with_list_adapter" />
</navigation>